{
  "@context": "https://schema.org",
  "@type": "Article",
  "headline": "[Article Title]",
  "description": "[Article description]",
  "author": {
    "@type": "Person",
    "name": "[Author Name]",
    "url": "[https://example.com/author]"
  },
  "datePublished": "[YYYY-MM-DD]",
